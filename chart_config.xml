<root>
    <chart 
        data_provider="Threshold_range"
        title="Boundaries Of Average Income"
        subtitle="Please Specify The Intervals"
        type="line"
        fillBetween="true"
    >
        <sliders>
            <slider type="yearSlider"/>
            <slider type="percentageSlider"/>
        </sliders>
        <datasets>
            <dataset
                dataset_id="min"
                label="A label"
                color="rgb(90, 30, 255)"
                borderWidth="1"
                fill="true"
            />

            <dataset
                dataset_id="max"
                label="A label"
                color="rgb(90, 20, 255)"
                borderWidth="1"
                fill="true"
            />
        </datasets>
        <!--  -->
        <options pointRadius="2" legendDisplay="true">
            <yAxis min="0" max="1" scaleLabel=""/>
            <xAxis scaleLabel=""/>
        </options>
    </chart>
</root>